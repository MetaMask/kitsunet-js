{
  "extends": [
    "tslint-config-standard",
    "tslint-jsdoc-rules"
  ],
  "rules": {
    "align": [
      true,
      "parameters",
      "statements",
      "arguments",
      "members",
      "elements"
    ],
    "space-before-function-paren": true,
    "no-multi-spaces": [
      true,
      {
        "exceptions": {
          "PropertyAssignment": false
        }
      }
    ]
  }
}